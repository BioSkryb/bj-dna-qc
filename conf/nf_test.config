params {
    input_csv                  = "$baseDir/tests/data/inputs/input.csv"
    publish_dir                = "results/bj-dna-qc"

    skip_kraken                = false
    skip_qualimap              = false
    skip_mapd                  = false
    skip_fastqc                = false
    skip_ginkgo                = false

    max_memory                 = 14.GB
    max_cpus                   = 4
    max_time                   = 2.h
}

process {
    containerOptions = "--env NF_TEST=true --env SENTIEON_LICENSE_SERVER=sentieon.bioskryb.com:8990"
}